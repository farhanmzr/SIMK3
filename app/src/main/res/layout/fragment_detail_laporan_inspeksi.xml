                    android:text="@string/lokasi"
                    android:textColor="@color/colorNeutral300"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_lokasi_apar"
                    android:layout_width="0dp"
                    android:layout_weight="1.6"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sf_pro_display_semibold"
                    android:gravity="end"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_gravity="center_vertical"
                    tools:text="Perpustakaan"
                    android:textColor="@color/colorNeutral500"
                    android:textSize="16sp" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sf_pro_display_medium"
                    android:text="@string/media"
                    android:textColor="@color/colorNeutral300"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_media_apar"
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sf_pro_display_semibold"
                    android:gravity="end"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_gravity="center_vertical"
                    tools:text="APAR Air"
                    android:textColor="@color/colorNeutral500"
                    android:textSize="16sp" />

            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.5"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sf_pro_display_medium"
                    android:text="@string/kondisi"
                    android:textColor="@color/colorNeutral300"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_kondisi_apar"
                    android:layout_width="0dp"
                    android:layout_weight="1.5"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/sf_pro_display_semibold"
                    android:gravity="end"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:layout_gravity="center_vertical"
                    tools:text="Baik"
                    tools:textColor="@color/colorTxtYellow"
                    android:textSize="16sp" />

            </TableRow>

        </TableLayout>

    </LinearLayout>

    <TextView
